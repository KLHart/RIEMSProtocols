<?xml version='1.0' encoding='utf-8'?>
<widget id="com.klevihart.riemsprotocols" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RI EMS Protocols 2017</name>
    <description>
        A basic application for the Rhode Island EMS protocols
    </description>
    <author email="kaolin.l.hart@gmail.com" href="">
        Kaolin Hart
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/icon.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreen" value="none" />
    <preference name="AutoHideSplashScreen" value="true" />
</widget>
